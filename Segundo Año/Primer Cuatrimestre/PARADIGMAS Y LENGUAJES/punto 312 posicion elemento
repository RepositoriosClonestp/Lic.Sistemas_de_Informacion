(defun posicion ()
	(let (a) (lista))
	(princ "ingrese la lista: ")
	(setq lista (read))
	(princ "ingrese el elemento cuya posicion se ha de buscar dentro de la lista: ")
	(setq a (read))
		(if (and (consp lista) (atom a))
			(print (position a lista))
			(print "El elemento no se encuentra dentro de la lista")
		)
)


