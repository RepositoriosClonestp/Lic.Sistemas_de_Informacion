(defun primerDia (max_temp)
	(pprint "¿Que temperatura se registra el primer dia del mes?")
	(princ (car max_temp)) 
	(terpri)
) 

(defun ultimoDia(max_temp)
	(pprint "¿Que temperatura se registra el ultimo dia del mes?")
	(princ (last max_temp))
	(terpri)
)

(defun tercerDia (max_temp)
	(pprint "¿Que temperatura se registra en el tercer dia del mes?")
	(princ (nth 2 max_temp))
	(terpri)
)

(defun anteultDia (max_temp) 
	(pprint "¿Que temperatura se registra el anteultimo dia del mes?")
	(princ (nth 4 max_temp))
)

(defun operar_temp ()
	(let (max_temp)
	(princ "Ingrese las maximas temperaturas del mes de Enero: ") 
	(setq max_temp (read))
	(if (consp max_temp) 
		(progn 
			(primerDia max_temp)
			(ultimoDia max_temp)
			(tercerDia max_temp)
			(anteultDia max_temp)
		)
	)
)
)

