(defun posicion ()
	(let (lista)
	(princ "ingrese una lista: ")
	(setq lista (read))
		(if (consp lista)
			(progn 
				(pprint (rotarDerecha lista))
				(pprint (rotarIzquierda lista))
			) 
		)
	)
)

(defun rotarDerecha (lista)
	(append (cdr lista) (list (first lista)))
)

(defun rotarIzquierda (lista)
	(append (last lista) (butlast lista))
)

