(defun clasifico_triangulos ()
	(let ((ladoa) (ladob) (ladoc))
	(princ "ingrese un valor para el lado a: ")
	(setq ladoa (read))
	(princ "ingrese un valor para el lado b: ")
	(setq ladob (read))
	(princ "ingrese un valor para el lado c: ")
	(setq ladoc (read))
