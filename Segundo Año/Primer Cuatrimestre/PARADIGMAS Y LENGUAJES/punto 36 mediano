(defun mediano (x y z)
	(let (
			(myMin (min x y z))
			(myMax (max x y z))
	     )
			(cond 
			((< myMin x myMax) x)
			((< myMin y myMax) y)
			((< myMin z myMax)z)
		)
   )
)

MEDIANO
Break 1 [3]> 
(defun ingresar-mediano ()
	(let ((x) (y) (z))
	(princ "Ingrese el primer valor numerico para calcular el valor mediano")
	(setq x (read))
	(princ "ingrese el segundo valor numerico para calcular el valor mediano")
	(setq y (read))
	(princ "ingrese el tercer valor numerico para calcular el valor mediano")
	(setq z (read))
		(if (and (numberp x) (numberp y) (numberp z))
		(mediano x y z)
		'error-de-variables
		)
	)
)

INGRESAR-MEDIANO
Break 1 [3]> 
(defun calcular-mediano ()
	(ingresar-mediano (x y z))
	(mediano (x y z))
)

