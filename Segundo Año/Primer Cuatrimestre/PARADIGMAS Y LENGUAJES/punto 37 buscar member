(defun buscar_temp ()
	(let ((a) (tempEn) (tempFeb))
	(princ "Ingrese el valor de la temperatura de ayer: ")
	(setq a (read))
	(princ "Ingrese el valor de las temperaturas del mes de Enero: ")
	(setq tempEn (read))
	(princ "Ingrese las temperaturas del mes de Febrero: ")
	(setq tempFeb (read))
		(cond 
			((member a tempEn) (princ "La temperatura de ayer se encuentra registrada en el mes de Enero"))
			((member a tempFeb) (princ "La temperatura de ayer se encuentra registrada en el mes de Febrero"))
			((T (print "No se registro la misma temperatura ni en enero ni en febrero")))
		)
	)
)