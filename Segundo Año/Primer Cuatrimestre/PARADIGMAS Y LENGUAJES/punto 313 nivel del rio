(defun ingesar_dispersion ()
	(let (alto) (bajo))
	(princ "ingrese el valor mas alto del rio: ")
	(setq alto (read))
	(princ "ingrese el valor mas bajo del rio: ")
	(setq bajo (read))
		(if (and (numberp alto) (numberp bajo))
			(clasificar_dispersion alto bajo)
			'error-de-variables
		)
	)

(defun clasificar_dispersion (alto bajo)
	(let( 
		(dispersion (- alto bajo))
		)
		(cond
			((< dispersion 30) (print "dias parejos" ))
			((> dispersion 100) (print "dias locos"))
			(t (pprint "dias normales")) 
		)
	)
)




