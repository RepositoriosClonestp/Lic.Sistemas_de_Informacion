(defun cant_alumnos (lista_alumnos)
	(princ "La cantidad de alumnos que hay es de: ")
	(princ (length lista_alumnos ))
	(terpri)
)

(defun nro_libreta (lista_alumnos)
	(princ "El numero de libreta del primer alumno de la lista es: ")
	(princ (caar lista_alumnos))
	(terpri) 
)

(defun nota_ultimo (lista_alumnos)
	(princ "La nota del ultimo alumno de la lista es: ")
	(princ (cadar (last lista_alumnos)))
	(terpri)
)

(defun regular_primero (lista_alumnos)
	(princ "El primer alumno de la lista ha regularizado la materia? ")
	(if (>= (cadar lista_alumnos) 6)
		(princ "Si regularizo la materia")
		(princ "no regularizo la materia")
	) 
)

(defun nota_alumnos ()
	(let ((lista_alumnos '()))
	(princ "Ingrese el numero de libreta universitaria y la nota de cada alumno: ")
	(setq lista_alumnos (read))
		(if (listp lista_alumnos)
			(progn 
				(cant_alumnos lista_alumnos)
				(nro_libreta  lista_alumnos)
				(nota_ultimo lista_alumnos)
				(regular_primero lista_alumnos)
			)
		)
	)
)

