(defun sirvepino ()
	(let ((altura)) 
	(princ "ingrese la altura del pino en metros: ")
	(setq altura (read))
		(if (numberp altura) 
			(esPesoUtil (pesoPino altura))
		)
	)
)

(defun pesoPino (altura)
	(let ((peso 0)) 
		(cond 
			((<= altura 3) (setq peso (*altura 300 )))
			((> altura 3) (setq peso (+ (* 3 300) (*200 (- altura 3)))))
			peso) 
	)
)

(defun esPesoUtil (peso)
	(if ((and (<= peso 400) (>= peso 1000)) princ "le sirve a la fabrica")
	)
)

